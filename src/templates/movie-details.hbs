{{#each this}}
<li class="result-details">
  <div class="result-detail-img-place">
    {{#if poster_path}}
    <img src="https://image.tmdb.org/t/p/original{{poster_path}}" alt="Movie poster" class="result-detail-img"
      id="{{id}}">
    {{else}}
    <img src="https://www.indulgemassager.com/assets/frontend/indulge/images/no-profile-pic.jpg" alt="Movie poster"
      class="result-detail-img" id="{{id}}">
    {{/if}}
  </div>
  {{!-- <img src="https://image.tmdb.org/t/p/original{{poster_path}}" alt="" class="result-detail-img" /> --}}
  <div class="result-movie-info-card">
    <p class="result-title">{{title}}</p>
    <div class="result-movie-param">
      <div class="vote">
        <p class="result-vote">Rating / Votes</p>
        <p class="result-vote-value">
          <span class="result-rating ">{{vote_average}}</span> / {{vote_count}}</p>
      </div>
      <div class="popularity">
        <p class="result-popularity">Popularity</p>
        <p class="result-popularity-value">{{popularity}}</p>
      </div>
      <div class="original-title">
        <p class="result-original-title">Original Title</p>
        <p class="result-original-title-value">{{original_title}}</p>
      </div>
      <div class="genre">
        <p class="result-gemre">Genre</p>
        <p class="result-gemre-value">{{genres}}</p>
      </div>
    </div>
    <p class="result-about">about</p>
    <p class="result-overview">{{overview}}</p>
    <div class="detailsBtn">
      <button class="details-button add-to-watch-button" data-action="add-to-watched">
        add to watched
      </button>
      <button class="details-button add-to-queue-button" data-action="add-to-queue">
        add to queue
      </button>
    </div>
  </div>
</li>
{{/each}}